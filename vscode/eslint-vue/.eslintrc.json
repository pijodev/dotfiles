{
  "extends": ["airbnb", "prettier", "plugin:vue/recommended"],

  "plugins": ["vue", "prettier"],

  "parserOptions": {
    "sourceType": "module"
  },

  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "no-unused-vars": "warn",
    "func-names": "off",
    "no-process-exit": "warn"
  }
}
